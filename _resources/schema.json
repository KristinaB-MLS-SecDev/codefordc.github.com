{
    "$schema": "http://json-schema.org/schema#",
    "type":"object",
    "properties":{
        "conformsTo":{
            "type":"string",
            "enum": [
                "http://codefordc.org/resources/specification.html"
            ]
        },
        "status":{
            "type":"string",
            "enum":[
                "Ideation",
                "Alpha",
                "Beta",
                "Production",
                "Archival"
            ]
        },
        "thumbnailUrl":{
            "type":"string"
        },
        "contact":{
            "type":"object",
            "properties":{
                "name":{
                    "type":"string"
                },
                "email":{
                    "type":"string"
                },
                "twitter":{
                    "type":"string"
                }
            },
            "additionalProperties":false
        },
        "geography":{
            "type":"string"
        },
        "politicalEntity":{
            "type":"object"
        },
        "governmentPartner":{
            "type":"object"
        },
        "communityPartner":{
            "type":"object"
        },
        "type":{
            "type":"string"
        },
        "data":{
            "type":"object"
        },
        "tags":{
            "type":"array",
            "items":{
                "type":"string"
            }
        },
        "moreInfo":{
            "type":"string"
        }
    },
    "required": ["conformsTo", "contact", "status"]
}
